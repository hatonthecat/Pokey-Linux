@charset "utf-8";

.wppb-user-forms, .wppb-user-forms * { box-sizing: border-box !important; }

.wppb-user-forms .wppb-form-field { list-style: none; }

.wppb-user-forms input:not([type=3D"button"]):not([type=3D"reset"]):not([ty=
pe=3D"submit"]):not([type=3D"file"]):not([value=3D"agree"]), .wppb-user-for=
ms select, .wppb-user-forms textarea { font-size: 100%; margin: 0px; vertic=
al-align: baseline; }

.wppb-user-forms input[type=3D"checkbox"], .wppb-user-forms input[type=3D"r=
adio"] { padding: 0px; }

.wppb-user-forms input[type=3D"search"] { appearance: textfield; box-sizing=
: content-box; }

.wppb-user-forms input[type=3D"search"]::-webkit-search-decoration { appear=
ance: none; }

.wppb-user-forms input[type=3D"text"], .wppb-user-forms input[type=3D"numbe=
r"], .wppb-user-forms input[type=3D"email"], .wppb-user-forms input[type=3D=
"url"], .wppb-user-forms input[type=3D"password"], .wppb-user-forms input[t=
ype=3D"search"], .wppb-user-forms textarea { color: rgb(102, 102, 102); bor=
der: 1px solid rgb(204, 204, 204); border-radius: 3px; }

.wppb-user-forms input[type=3D"text"]:focus, .wppb-user-forms input[type=3D=
"number"]:focus, .wppb-user-forms input[type=3D"email"]:focus, .wppb-user-f=
orms input[type=3D"url"]:focus, .wppb-user-forms input[type=3D"password"]:f=
ocus, .wppb-user-forms input[type=3D"search"]:focus, .wppb-user-forms texta=
rea:focus { color: rgb(17, 17, 17); }

.wppb-user-forms input[type=3D"text"], .wppb-user-forms input[type=3D"numbe=
r"], .wppb-user-forms input[type=3D"email"], .wppb-user-forms input[type=3D=
"url"], .wppb-user-forms input[type=3D"password"], .wppb-user-forms input[t=
ype=3D"search"] { padding: 3px; }

.wppb-user-forms textarea { overflow: auto; padding-left: 3px; vertical-ali=
gn: top; width: 100%; }

.wppb-user-forms .wppb-wysiwyg .wp-editor-wrap { float: left; width: 69.9%;=
 }

.wppb-user-forms .wppb-wysiwyg button:hover { background: none; }

.wppb-user-forms .wppb-wysiwyg div.mce-tinymce.mce-panel { border: 1px soli=
d rgb(204, 204, 204) !important; color: rgb(102, 102, 102) !important; }

.wppb-user-forms .wppb-wysiwyg div.mce-panel.mce-first { border-width: 0px =
0px 1px !important; }

