+#############################################################
+LZMA_VERSION:=4.43
+LZMA_SOURCE:=lzma-$(LZMA_VERSION).tar.bz2
+LZMA_SITE:=http://sources.openbricks.org/3.1
+LZMA_DIR:=$(BUILD_DIR)/lzma-$(LZMA_VERSION)
+LZMA_BINARY:=lzma
+LZMA_TARGET_BINARY:=usr/bin/lzma
+LZMA_TARGET_CXX:=$(STAGING_DIR)/bin/$(ARCH)-linux-g++
+
+$(DL_DIR)/$(LZMA_SOURCE):
+	$(WGET) -P $(DL_DIR) $(LZMA_SITE)/$(LZMA_SOURCE)
+
+$(LZMA_DIR)/.source: $(DL_DIR)/$(LZMA_SOURCE)
+	bzcat $(DL_DIR)/$(LZMA_SOURCE) | tar -C $(BUILD_DIR) $(TAR_OPTIONS) -
