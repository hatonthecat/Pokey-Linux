++
++#define SG_MAX_SENSE 16   /* this only applies to the sg_header interface */
++
++struct sg_header
++ {
++   /* Length of incoming packet (including header).  */
++   int pack_len;
++   /* Maximal length of expected reply.  */
++   int reply_len;
++   /* Id number of packet.  */
++   int pack_id;
